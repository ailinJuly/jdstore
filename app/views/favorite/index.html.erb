<h2>我的收藏</h2>
<table class="table table-bordered">
    <thead>
        <tr>
            <th width="80%">商品明细</th>
            <th>单价</th>
        </tr>
    </thead>
    <tbody>
        <% @products .each do |product| %>
            <tr>
                <td>
                    <%= link_to(product.title, product_path(product))  %>
                </td>
                <td>
                    <%= product.price %>
                </td>
            </tr>
        <% end %>
    </tbody>
</table>
